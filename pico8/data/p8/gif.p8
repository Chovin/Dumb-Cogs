version 42
__lua__
cls();while time()<1 do;flip();end
{setup_code}
__wait_to_record__=true
while time() < {length} do
    {draw_code}
    if ({do_flip}) flip()
    if {do_record} and __wait_to_record__ and time() >= {wait} then 
      extcmd("rec")
      __wait_to_record__=nil
    end
end
if ({do_record}) extcmd("video")
